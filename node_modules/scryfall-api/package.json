{
	"name": "scryfall-api",
	"version": "3.2.5",
	"description": "A Javascript library for https://scryfall.com/docs/api written in Typescript.",
	"keywords": [
		"magic the gathering",
		"magic",
		"gathering",
		"api",
		"mtg",
		"typescript",
		"card",
		"cards",
		"scryfall",
		"scry"
	],
	"homepage": "https://github.com/MarioMH8/scryfall-api",
	"bugs": {
		"url": "https://github.com/MarioMH8/scryfall-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MarioMH8/scryfall-api"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"module": "./dist/scryfall-api.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/scryfall-api.js",
			"import": "./dist/scryfall-api.mjs",
			"default": "./dist/scryfall-api.mjs"
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"publish": "yarn changeset publish",
		"test": "vitest run --no-isolate",
		"test:coverage": "vitest run --no-isolate --coverage",
		"typecheck": "tsc --noEmit",
		"version": "yarn changeset version && yarn install --mode update-lockfile"
	},
	"peerDependencies": {
		"axios": "^0 || ^1"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.1",
		"@hexatool/eslint-config-typescript": "1.0.9",
		"@hexatool/tsconfig": "20.0.1",
		"@types/node": "20.2.5",
		"@vitest/coverage-c8": "0.31.2",
		"axios": "1.4.0",
		"eslint": "8.41.0",
		"typescript": "5.0.4",
		"vite": "4.3.9",
		"vite-plugin-dts": "2.3.0",
		"vite-plugin-externalize-deps": "0.6.0",
		"vitest": "0.31.2"
	},
	"volta": {
		"node": "18.16.0",
		"yarn": "3.5.1"
	},
	"packageManager": "yarn@3.5.1"
}
